grammar Arithmetic
  rule additive
    multitive '+' additive <AdditiveNode>
    /
    multitive
  end

  rule multitive
    primary '*' multitive <MultitiveNode>
    /
    primary
  end

  rule primary
    '(' additive ')' <PrimaryNode>
    /   
    number
  end

  rule number
    first_digit:[1-9] rest_digits:[0-9]* <NumberNode>
  end
end
